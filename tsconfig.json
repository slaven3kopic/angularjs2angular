{
    "compilerOptions": {
        "baseUrl": "",
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es6",
            "dom"
        ],
        "mapRoot": "./",
        "module": "es6",
        "moduleResolution": "node",
        "outDir": "../dist/out-tsc",
        "sourceMap": true,
        "target": "es5",
        "typeRoots": [
            "../node_modules/@types"
        ],
        "allowSyntheticDefaultImports": true
    },
    "paths": {
        "core-js/es7/reflect": [
            "../node_modules/core-js/proposals/reflect-metadata"
        ]
    }
}
